{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "Function.OtaCommandStart": "Trace",
      "Function.OtaCommandOrchestrator": "Trace",
      "Function.OtaCommandSubmit": "Trace",
      "Function.OtaCommandSending": "Trace",
      "Function.OtaCommandDelivery": "Trace",
      "Function.OtaCommandResponse": "Trace",
      "default": "Information"
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[1.*, 2.0.0)"
  },
  "extensions": {
    "durableTask": {
      "localRpcEndpointEnabled": false
    }
  }
}
