{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Current File",
      "program": "${file}",
      "outputCapture": "std",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Attach to data-transformer",
      "type": "node",
      "request": "attach",
      "port": 9230,
      "preLaunchTask": "func: host start --port 7072"
    }
  ]
}