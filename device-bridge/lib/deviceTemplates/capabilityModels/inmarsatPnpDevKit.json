            "@id": "urn:inmarsat:idp:devKit:feuIdp:commands:configSet:1",
            "@type": "Command",
            "durable": true,
            "name": "configSet",
            "displayName": {
              "en": "Set Properties"
            },
            "description": "Allows configuration of application settings.",
            "request": {
              "@type": "CommandPayload",
              "name": "configProperties",
              "displayName": "Configuration Properties",
              "comment": "Interval valid range [0..86400] seconds.",
              "schema": {
                "@id": "urn:inmarsat:idp:devKit:feuIdp:commands:configSet:configProperties:1",
                "@type": "Object",
                "displayName": "Object",
                "fields": [
                  {
                    "@id": "urn:inmarsat:idp:devKit:feuIdp:commands:configSet:configProperties:reportInterval:1",
